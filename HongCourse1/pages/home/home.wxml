<!--pages/home/home.wxml-->
<view class="container-home">
  <image class="home-bg" src="/images/home-bg.png" >
  </image>
  <swiper indicator-dots="{{indicatorDots}}" class="swiper"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
  <view class="topu-font">托普教育</view>
  <view class="weui-cells weui-cells_after-title">
      <view class="weui-cell weui-cell_input">
        <input class="weui-input" maxlength="10" placeholder="最大输入长度为10" />
      </view>
    </view>
  <view class="courses">
    <view class="course">
      <image class="img-course"  src="/images/oversea.png"></image>
      <view>海外留学</view>
    </view>
    <view class="course">
      <image class="img-course" mode="aspectFit" src="/images/senior.png"></image>
      <view>高中课程</view>
    </view>
    <view class="course">
      <image class="img-course" mode="aspectFit" src="/images/reread.png"></image>
      <view>中复班</view>
    </view>
    <view class="course">
      <image class="img-course" mode="aspectFit" src="/images/junior.png"></image>
      <view>初中课程</view>
    </view>
  </view>
</view>


